     <v-form v-model=\"valid\">\n                            <v-container>\n                                <v-row>\n                                    <v-col cols=\"12\">\n                                        <v-text-field v-model=\"email\" :rules=\"emailRules\" label=\"E-mail\" :error=\"error\" :error-messages=\"error_messages[0]\" required></v-text-field>\n                                    </v-col>\n                                </v-row> \n                                <v-row>\n                                    <v-col cols=\"12\">\n                                        <v-text-field v-model=\"password\" :rules=\"nameRules\" label=\"password\" type=\"password\" :error=\"error\" :error-messages=\"error_messages[1]\" required></v-text-field>\n                                    </v-col>\n                                </v-row>\n                            </v-container>\n                            <v-spacer></v-spacer>\n                            <v-card-actions>\n                                <v-spacer></v-spacer>\n                                <v-btn text icon color=\"primary\" @click=\"login()\">\n                                    Submit\n                                    <v-icon small>far fa-paper-plane</v-icon>\n                                </v-btn>\n                            </v-card-actions>\n                        </v-form>\n                    </v-card>\n                </v-flex>\n            </v-layout>\n        </v-container>\n    </transition>\n</template>\n<style>\n    .bgc{\n        background-color: #5165a3;\n    }\n</style>\n<script>\n    const axios = require('axios');\n    export default {\n    name: 'login',\n    data () {\n          return {\n            email: '',\n            password: '',\n            valid: false,\n            error: false,\n            nameRules: [\n                v => !!v || 'password is required',\n                v => v.length <= 20 || 'Name must be less than 10 characters',\n              ],\n              email: '',\n              emailRules: [\n                v => !!v || 'E-mail is required',\n                v => /.+@.+/.test(v) || 'E-mail must be valid',\n              ]\n            }\n        },\n        methods: {\n          login(){\n//write login authencation logic here!\n              var vm = this;\n              axios.post('/loginCheck', {\n                username: vm.email,\n                password: vm.password\n              })\n              .then(function (response) {\n                localStorage.setItem('token', 'ImLogin');\n                vm.error = false;\n                vm.$router.push('/');\n              })\n              .catch(function (error) {\n                vm.error = true;\n                console.log(error);\n              });\n              \n              /*\n                if( this.email == 'm951406@gmail.com' && this.password == '12342234' ){\n                  localStorage.setItem('token', 'ImLogin');\n                  this.error = false;\n                  this.$router.push('/');\n                } else{\n                  this.error = true\n                }\n              */\n              }\n        },\n        computed : {\n            error_messages () {\n                if(this.error == true){\n                    return [\n                          \"E-mail is incorrect\",\n                          \"password is incorrect\"\n                      ]\n                }\n                else return []\n            }\n        },\n        \n        mounted () {\n            this.error = false;\n        }\n    }\n</script>"]}]}