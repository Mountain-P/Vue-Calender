AACA,KAJA;AAKA,IAAA,gBALA,8BAKA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,WAAA,SAAA;AACA,KARA;AASA,IAAA,SATA,uBASA;AAAA;;AACA,MAAA,KAAA,CACA,IADA,CACA,YADA,EACA;AACA,QAAA,QAAA,EAAA,YAAA,CAAA,OAAA,CAAA,UAAA;AADA,OADA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,OATA,EAUA,KAVA,CAUA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAZA,EADA,CAcA;AACA,KAxBA;AAyBA,IAAA,MAzBA,oBAyBA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,UAAA;AACA,MAAA,KAAA,CACA,IADA,CACA,SADA,EACA;AACA,QAAA,QAAA,EAAA,YAAA,CAAA,OAAA,CAAA,UAAA,CADA;AAEA,QAAA,YAAA,EAAA,KAAA,UAFA;AAGA,QAAA,SAAA,EAAA,KAAA;AAHA,OADA,EAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,YAAA,GAAA,KAAA;AACA,OATA,EAUA,KAVA,CAUA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAZA,EAFA,CAeA;AACA;AAzCA;AAdA,CAAA","sourcesContent":["<template>\n  <div>\n    <transition name=\"fade\">\n      <!--\n        <router-link to=\"/\">Home</router-link>\n        <router-link to=\"/user\">User</router-link>\n        <a href=\"\" @click.prevent=\"logout\">Logout</a>\n      -->\n\n      <v-app-bar app color=\"#5165a3\" dark>\n        <div class=\"d-flex align-center\">\n          <v-btn icon color=\"#5165a3\">\n            <v-icon color=\"white\">far fa-calendar</v-icon>\n          </v-btn>\n          <v-btn small depressed color=\"#5165a3\">calendar</v-btn>\n        </div>\n\n        <v-spacer></v-spacer>\n\n        <v-avatar style=\"marginRight:15px\" size=\"36\" color=\"#d996cc\">\n          <v-menu v-model=\"user_toggle\" :close-on-content-click=\"false\" offset-y offset-x>\n            <template v-slot:activator=\"{ on }\">\n              <v-btn icon text @click=\"user_toggle = true\" v-on=\"on\">\n                <span class=\"white--text\">{{userName}}</span>\n              </v-btn>\n            </template>\n\n            <v-card class=\"mx-auto\" max-width=\"400\">\n              <v-img\n                class=\"white--text align-end\"\n                height=\"200px\"\n                width=\"300px\"\n                src=\"https://cdn.vuetifyjs.com/images/cards/docks.jpg\"\n              >\n                <v-container fluid style=\"height: 100%;\">\n                  <v-layout row justify-center align-center style=\" height: 100%\">\n                    <v-flex shrink>\n                      <v-avatar size=\"72\" color=\"#d996cc\" style=\"marginBottom:60px\">\n                        <span class=\"white--text\">{{userName}}</span>\n                      </v-avatar>\n                    </v-flex>\n                  </v-layout>\n                </v-container>\n              </v-img>\n\n              <v-card-title class=\"pb-0\">{{userName}}</v-card-title>\n\n              <v-card-text class=\"text--primary\">\n                <div>歡迎使用本系統,您可以</div>\n              </v-card-text>\n\n              <v-card-actions class=\"justify-center\">\n                <v-btn color=\"red\" text @click=\"openModifyDialog()\">修改個人資料</v-btn>\n                <v-btn color=\"red\" text @click=\"logout()\">登出</v-btn>\n              </v-card-actions>\n            </v-card>\n          </v-menu>\n        </v-avatar>\n      </v-app-bar>\n    </transition>\n    <v-row justify=\"center\">\n      <v-dialog v-model=\"modifyDialog\" persistent max-width=\"600px\">\n        <v-card>\n          <v-card-title>\n            <span class=\"headline\">修改資料</span>\n          </v-card-title>\n          <v-card-text>\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\">\n                  <v-text-field v-model=\"modifyName\" label=\"帳號名稱\" :rules=\"modifyNameRules\" required></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field\n                    v-model=\"modifyEmail\"\n                    label=\"電子郵件\"\n                    :rules=\"modifyEmailRules\"\n                    required\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\">\n                  <v-text-field\n                    v-model=\"modifyPass\"\n                    label=\"密碼\"\n                    type=\"password\"\n                    :rules=\"modifyPassRules\"\n                    required\n                  ></v-text-field>\n                </v-col>\n              </v-row>\n            </v-container>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"modifyDialog=false\">取消</v-btn>\n            <v-btn color=\"blue darken-1\" text @click=\"modify()\">送出</v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-dialog>\n    </v-row>\n  </div>\n</template>\n\n<script>\nconst axios = require(\"axios\");\nexport default {\n  data: () => {\n    return {\n      modifyDialog: false,\n      modifyName: \"\",\n      modifyPass: \"\",\n      modifyEmail: \"\",\n      modifyNameRules: [v => !!v || \"請輸入使用者帳號\"],\n      modifyPassRules: [v => !!v || \"請輸入使用者密碼\"],\n      modifyEmailRules: [v => !!v || \"請輸入使用者信箱\"],\n      user_toggle: false,\n      userName: localStorage.getItem(\"username\")\n    };\n  },\n  methods: {\n    logout() {\n      localStorage.removeItem(\"token\");\n      this.$router.push(\"/login\");\n    },\n    openModifyDialog() {\n      this.modifyDialog = true;\n      this.getModify();\n    },\n    getModify() {\n      axios\n        .post(\"/getModify\",{\n          userName: localStorage.getItem('username'),\n        })\n        .then(response => {\n          this.modifyName=response.data.name;\n          this.modifyPass=response.data.password;\n          this.modifyEmail=response.data.email;\n          console.log(response.data);\n        })\n        .catch(response => {\n          console.log(error);\n        });\n      //console.log(this.selectedEvent);\n    },\n    modify() {\n      console.log(this.modifyPass);\n      axios\n        .post(\"/modify\",{\n          userName: localStorage.getItem('username'),\n          userPassword:this.modifyPass,\n          userEmail:this.modifyEmail\n        })\n        .then(response => {\n          console.log(response.data);\n          this.modifyDialog=false;\n        })\n        .catch(error => {\n          console.log(error);\n        });\n      //console.log(this.selectedEvent);\n    }\n    }\n  \n};\n</script>"],"sourceRoot":"src/components"}]}